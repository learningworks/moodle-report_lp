{{!
    @template report_lp/summary_report

    This template renders the main content area for the myoverview block.

    Example context (json):
    {}
}}
<nav aria-label="Page navigation">
    {{#pagination}}
        <ul class="pagination pagination-md">
            {{#previouspage}}
                <li class="page-item {{#disabled}}disabled{{/disabled}}">
                    <a class="page-link" href="{{url}}" tabindex="-1">{{text}}</a>
                </li>
            {{/previouspage}}
            {{#pages}}
                <li class="page-item {{#active}}active{{/active}}"><a class="page-link" href="{{url}}">{{text}}</a></li>
            {{/pages}}
            {{#nextpage}}
                <li class="page-item {{#disabled}}disabled{{/disabled}}">
                    <a class="page-link" href="{{url}}" tabindex="-1">{{text}}</a>
                </li>
            {{/nextpage}}
        </ul>
    {{/pagination}}
</nav>